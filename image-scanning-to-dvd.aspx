  <div class="container hdrBanner">
    <div class="row" id="povbanner">
      <div class="col-xs-12">
        <div class="outer povbanner-outer">
          <div class="inner povbanner-inner">
            <img src="/img/global/hdr/blueBg.jpg" alt="" />
            <h1>Image Scanning to DVD</h1>
          </div>
        </div>
      </div>
    </div>
    <div class="row" id="infront">
      <div class="col-xs-12">
        <div class="outer infront-outer">
          <div class="inner infront-inner">
            <img src="/img/global/hdr/image-scanning-to-dvd.png" alt="" />
            <div class="btnHdrOrder">
              <button type="button" class="btnOrange btnSm" data-reveal-id="ordernowmodal">Order Now <img src="/img/global/imgArrowWhite.png" class="arrowwhite" /></button>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div><!-- end: hdrBanner -->
  <div class="clearfix"></div>

  <div class="container" id="info">
    <div class="row" id="format">
      <div class="col-xs-12">
        <div class="steptext">
          <h2>Accepted Formats</h2>
        </div>
      </div>
      <div class="col-xs-12 col-sm-4">
        <div class="steptext">
          <h4>35mm Slides</h4>
          <div class="stepimg">
            <img src="/img/services/formats_images_slides.jpg" alt="Formats accepted: 35mm Slides, Negatives, Photo Prints" />
          </div>
          <div id="label-table">
            <div id="space1" class="image-format-label h5">cardboard</div>
            <div id="space2" class="image-format-label h5">plastic</div>
          </div>
        </div>
      </div>
      <div class="col-xs-12 col-sm-4">
        <div class="steptext">
          <h4>Negatives</h4>
          <div class="stepimg">
            <img src="/img/services/formats_images_negatives.jpg" alt="Formats accepted: 35mm Slides, Negatives, Photo Prints" />
          </div>
          <h5>35mm, APS film, 110 film, 110/220 film, 126 "Instamatic", large negatives<br />*Negative transfers available online only</h5>
        </div>
      </div>
      <div class="col-xs-12 col-sm-4">
        <div class="steptext">
          <h4>Photo Prints</h4>
          <div class="stepimg">
            <img src="/img/services/formats_images_photos.jpg" alt="Formats accepted: 35mm Slides, Negatives, Photo Prints" />
          </div>
          <h5>from 2" x 2" to 8" x 12"</h5>
        </div>
      </div>
    </div><!-- end: format -->
    <div class="clearfix"></div>

    <div class="row" id="pricing">
      <div class="col-xs-12">
        <div class="steptext">
          <h2>Image Scanning to DVD Pricing</h2>
          <h4>
            <span class="h3">$19.99</span> for the first 40 slides/prints/negatives<br />
            <span class="h3">$0.39</span> per slides/prints/negatives after 40
          </h4>
          <h4><b>Formats:</b> all slides (no glass mounts), photo prints up to 8" x 12", photo negatives</h4>
          <h4><b>Maximum:</b> 1600 images</h4>
        </div>
      </div>
    </div><!-- end: pricing -->
    <div class="clearfix"></div>

    <div class="row" id="whatyouget">
      <div class="col-xs-12">
        <div class="steptext">
          <h2>What You Get</h2>
        </div>
      </div>
      <div class="col-xs-12 col-sm-4">
        <div class="steptext">
          <div class="stepimg">
            <img src="/img/services/dvd-personalized.png" alt="" />
          </div>
          <h5>Personalized Data DVD</h5>
        </div>
      </div>
      <div class="col-xs-12 col-sm-4">
        <div class="steptext">
          <div class="stepimg">
            <img src="/img/services/chapter-menus.png" alt="" />
          </div>
          <h5>DVD Chapter Menus</h5>
        </div>
      </div>
      <div class="col-xs-12 col-sm-4">
        <div class="steptext">
          <div class="stepimg">
            <img src="/img/services/video-highlights.png" alt="" />
          </div>
          <h5>Music Video Highlights</h5>
        </div>
      </div>
    </div><!-- end: whatyouget -->
    <div class="clearfix"></div>

    <script type="text/javascript">wOrderNow();</script>
    
  </div><!-- end: #info -->

  <script type="text/javascript">wFooter();</script>
  
</div><!-- end: .container-fluid -->

<!-- reveal modals -->
<script type="text/javascript">
  wOrderNowModal();
  wGetOffers();
</script>

